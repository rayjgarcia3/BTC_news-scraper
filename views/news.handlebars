<div class="boombox">
  <div class="row center">
     <br><br>

     <h1 class="white-text">Latest and greatest crypto-news!</h1>
  </div>
</div>
<div class="container">
  <div class="section">
    <div class="row">
      <ul id="reports" class="collection">
        {{#each reports}}
          <li class="collection-item">{{this.title}}<br /><a href="{{this.link}}">Read more</a></li>
        {{/each}}
      </ul>
    </div>
  </div>
</div>
